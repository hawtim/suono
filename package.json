{
  "name": "suono",
  "version": "0.1.4",
  "description": "A audio library for applications.",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "keywords": [
    "audio",
    "player",
    "typescript",
    "media",
    "application",
    "spa",
    "HTMLAudioElement",
    "HTMLMediaElement"
  ],
  "scripts": {
    "prebuild": "del-cli dist",
    "esm": "tsc --module esnext && cpy dist/index.js dist --rename index.esm.js",
    "cjs": "tsc --module commonjs",
    "build": "npm run esm && npm run cjs",
    "test": "ava",
    "dev": "npm run build --watch",
    "lint": "xo --fix"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hawtim/suono.git"
  },
  "author": "hawtim<hawtimzhang@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hawtim/suono/issues"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "homepage": "https://github.com/hawtim/suono#readme",
  "dependencies": {
    "ava": "^3.13.0",
    "cpy-cli": "^3.1.1",
    "del-cli": "^3.0.1",
    "jsdom": "^16.4.0",
    "xo": "^0.33.1"
  }
}
